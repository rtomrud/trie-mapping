{
  "name": "trie-mapping",
  "version": "3.0.0-rc.0",
  "description": "A compact trie for mapping keys to values that allows efficient prefix-based retrievals",
  "keywords": [
    "compact",
    "dictionary",
    "map",
    "prefix",
    "radix",
    "suffix",
    "tree",
    "trie"
  ],
  "homepage": "https://github.com/rtomrud/trie-mapping#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/trie-mapping/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "dist",
    "index.js"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/trie-mapping.git"
  },
  "scripts": {
    "prebench": "npm run build -s",
    "bench": "npm run bench-payload -s && npm run bench-space -s && npm run bench-time -s",
    "bench-payload": "node bench/payload/map.js && node bench/payload/trie.js",
    "bench-space": "node --expose-gc bench/space/map.js && node --expose-gc bench/space/trie.js",
    "bench-time": "node bench/time/bench.js",
    "build": "rollup -i index.js -o dist/index.js -f cjs",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "nyc --reporter html tape -r esm index.test.js && nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "coveralls": "3.0.11",
    "eslint": "6.8.0",
    "eslint-config-pretty-strict": "4.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "esm": "3.2.25",
    "husky": "4.2.3",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "rollup": "2.1.0",
    "tape": "4.13.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "./node_modules/eslint-config-pretty-strict/index.js"
    ],
    "overrides": [
      {
        "files": [
          "./bench/**/*.js"
        ],
        "env": {
          "node": true
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "root": true
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "esm": {
    "sourceMap": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
